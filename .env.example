# Default target used when --target flag is not specified (e.g. staging-settings, production-settings)
CRE_TARGET=

# CRE account credentials (for Playwright browser auth in TUI tests)
# Sign up at https://cre.chain.link
CRE_USER_NAME=
CRE_PASSWORD=

